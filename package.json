{
	"name": "longship",
	"version": "0.0.3",
	"description": "Sails-included starter based on H3, Preact, Tailwind CSS and Vike.",
	"scripts": {
		"dev": "cross-env NODE_ENV=development tsx ./src/server/entry-server.ts",
		"start": "cross-env NODE_ENV=production node ./dist/server/entry-server.mjs",
		"build": "rimraf ./dist && vite build --ssrManifest && vite build --ssr",
		"format:check": "biome format .",
		"format:fix": "biome format . --write",
		"lint": "biome lint .",
		"lint:fix": "biome lint . --apply",
		"test": "vitest"
	},
	"dependencies": {
		"@mantine/core": "^7.8.1",
		"@mantine/dates": "^7.9.0",
		"@mantine/dropzone": "^7.9.0",
		"@mantine/form": "^7.9.0",
		"@mantine/hooks": "^7.8.1",
		"@mantine/modals": "^7.9.0",
		"@mantine/notifications": "^7.9.0",
		"@mantinex/mantine-logo": "^1.0.1",
		"@preact/signals": "^1.2.3",
		"@tabler/icons-react": "^3.3.0",
		"cross-env": "^7.0.3",
		"h3": "^1.11.1",
		"pathe": "^1.1.2",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-streaming": "^0.3.28",
		"sirv": "^2.0.4",
		"vike": "^0.4.171",
		"vike-react": "^0.4.8"
	},
	"type": "module",
	"devDependencies": {
		"@babel/core": "^7.24.5",
		"@babel/plugin-transform-react-jsx": "^7.23.4",
		"@biomejs/biome": "^1.7.3",
		"@flydotio/dockerfile": "^0.5.7",
		"@testing-library/dom": "^10.1.0",
		"@testing-library/jest-dom": "^6.4.5",
		"@testing-library/react": "^15.0.7",
		"@testing-library/user-event": "^14.5.2",
		"@types/node": "^20.12.10",
		"@types/react": "^18.3.1",
		"@types/react-dom": "^18.3.0",
		"@vitejs/plugin-react-swc": "^3.6.0",
		"autoprefixer": "^10.4.19",
		"globals": "^15.1.0",
		"jsdom": "^24.0.0",
		"postcss": "^8.4.38",
		"postcss-preset-mantine": "^1.15.0",
		"postcss-simple-vars": "^7.0.1",
		"rimraf": "^5.0.5",
		"tsx": "^4.9.3",
		"typescript": "^5.4.5",
		"vite": "^5.2.11",
		"vite-plugin-compression2": "^1.1.0",
		"vitest": "^1.6.0"
	},
	"engines": {
		"node": "*",
		"bun": "*"
	},
	"bin": {
		"longship": "./dist/server/entry-server.mjs"
	}
}
