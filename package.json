{
	"name": "longship",
	"version": "0.0.3",
	"description": "Sails-included starter based on H3, Preact, Tailwind CSS and Vike.",
	"scripts": {
		"dev": "cross-env NODE_ENV=development tsx ./src/server/entry-server.ts",
		"start": "cross-env NODE_ENV=production node ./dist/server/entry-server.mjs",
		"build": "vite build --ssrManifest && vite build --ssr --target node18",
		"format:check": "biome format .",
		"format:fix": "biome format . --write",
		"lint": "biome lint .",
		"lint:fix": "biome lint . --apply",
		"test": "vitest"
	},
	"dependencies": {
		"@preact/signals": "^1.2.3",
		"cross-env": "^7.0.3",
		"h3": "^1.11.1",
		"pathe": "^1.1.2",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-streaming": "^0.3.28",
		"sirv": "^2.0.4",
		"vike": "^0.4.171",
		"vike-react": "^0.4.8"
	},
	"type": "module",
	"devDependencies": {
		"@babel/core": "^7.17.10",
		"@babel/plugin-transform-react-jsx": "^7.17.3",
		"@biomejs/biome": "1.7.0",
		"@flydotio/dockerfile": "^0.5.5",
		"@mantine/core": "^7.8.1",
		"@mantine/hooks": "^7.8.1",
		"@testing-library/react": "^15.0.5",
		"@types/eslint__js": "^8.42.3",
		"@types/node": "^20.12.7",
		"@vitejs/plugin-react": "^4.2.1",
		"autoprefixer": "^10.4.19",
		"globals": "^15.0.0",
		"jsdom": "^24.0.0",
		"postcss": "^8.4.38",
		"postcss-preset-mantine": "^1.15.0",
		"postcss-simple-vars": "^7.0.1",
		"tsx": "^4.7.2",
		"typescript": "^5.5.0-dev.20240427",
		"vite": "^5.2.9",
		"vitest": "^1.5.0"
	},
	"engines": {
		"node": "*",
		"bun": "*"
	},
	"bin": {
		"longship": "./dist/server/entry-server.mjs"
	}
}
